import { getCollection } from "astro:content";

export const getSinglePage = async (collection: any) => {
  const allPage = await getCollection(collection);
  const removeIndex = allPage.filter((data) => data.id.match(/^(?!-)/));
};

export const getSingleModel = async (collection: any) => {
  const allModels = await getCollection(collection);
  const removeIndex = allModels.filter((data) => data.id.match(/^(?!-)/));
};
